.page-oil-price {
  padding-bottom: 80rpx;
}
.page-oil-price-title {
  height: 92rpx;
  line-height: 92rpx;
  text-align: center;
  font-size: 24rpx;
  color: #b6b6b6;
}
.page-oil-price-table-th {
  display: flex;
  background-color: #fc4444;
  color: white;
  position: -webkit-sticky;
  position: sticky;
  top: 0px;
}
.page-oil-price-table-tr {
  display: flex;
  flex-direction: row-reverse;
}
.page-oil-price-table-td {
  width: 20%;
  padding: 20rpx 0;
  text-align: center;
  border-bottom: 1px solid #e5e5e5;
}
.page-oil-price-table .placeName {
  background-color: #ff8902;
  color: white;
}
