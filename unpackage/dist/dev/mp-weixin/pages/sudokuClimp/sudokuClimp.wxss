.sudoku-climp-container .climp-imgs-wrapper {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  margin-bottom: 40rpx;
}
.sudoku-climp-container .climp-imgs-wrapper image {
  width: 32%;
  margin-bottom: 1%;
}
.sudoku-climp-container button {
  margin: 40rpx auto;
  width: 90%;
}
.sudoku-climp-container #sudokuSource {
  width: 100%;
  opacity: 0;
  position: absolute;
}
.sudoku-climp-container canvas[id^="img-"] {
  width: 100px;
  height: 100px;
  border: 1px solid red;
  margin-bottom: 20rpx;
}
.sudoku-climp-container .adpater-title {
  padding-left: 5%;
  font-weight: bold;
  font-size: 40rpx;
  color: #333;
}
.sudoku-climp-container .bks-wrapper {
  display: flex;
  justify-content: space-around;
  margin-top: 40rpx;
}
.sudoku-climp-container .bks-wrapper .bks-item {
  width: 23vw;
  background-size: cover;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.sudoku-climp-container .bks-wrapper .bks-item image {
  width: 100%;
  height: 100%;
}
