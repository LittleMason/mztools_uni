.sudoku-climp-container button {
  margin: 40rpx auto;
  width: 90%;
}
.sudoku-climp-container #sudokuSource {
  width: 100%;
}
.sudoku-climp-container canvas[id^="img-"] {
  width: 100px;
  height: 100px;
  border: 1px solid red;
  margin-bottom: 20rpx;
}
.sudoku-climp-container .bks-wrapper {
  display: flex;
  justify-content: space-around;
  margin-top: 40rpx;
}
.sudoku-climp-container .bks-wrapper .bks-item {
  width: 23vw;
  height: 23vw;
  background-size: cover;
}
.sudoku-climp-container .bks-wrapper .bks-item image {
  width: 100%;
  height: 100%;
}
